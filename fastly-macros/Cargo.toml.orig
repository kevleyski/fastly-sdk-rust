[package]
name = "fastly-macros"
description = "Fastly Compute@Edge procedural macro implementation"
version.workspace = true
authors = ["Fastly <oss@fastly.com>"]
license = "Apache-2.0 WITH LLVM-exception"
edition = "2021"
categories = ["wasm"]
keywords = ["fastly"]

[lib]
proc-macro = true

[dependencies]
proc-macro2 = { version = "1.0", features = ["span-locations"] }
quote = "1.0"
syn = { version = "1.0", features = ["full"] }

[dev-dependencies]
#fastly = { workspace = true }
